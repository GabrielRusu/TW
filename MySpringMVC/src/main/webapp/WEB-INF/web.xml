		<init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>MySpringMVC</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>VillageView</servlet-name>
		<url-pattern>/viewVillage</url-pattern>
	</servlet-mapping>

    <servlet>
        <servlet-name>EarthquakeView</servlet-name>
